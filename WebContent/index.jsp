<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title>Washonn Franchise Enquire</title>
<meta charset="UTF-8">
<link rel="stylesheet" type="text/css" href="css/main.css">
</head>
<body>
	<div>
		<div class="container-contact50"></div>
		<div class="container-contact100">
			<div class="wrap-contact100 relative">
				<h3> Washonn Franchise Enquire </h3>
				<div class="wrap-input100 bg1">* Required</div>
				<form action="query_form" method="post">

					<div class="wrap-input100">
						<span>First Name <span class="bg1">*</span></span> <input
							class="input100" type="text" name="firstName"
							style="border-bottom: 1px solid #555555; width: 30%"
							placeholder="Your answer">
					</div>

					<div class="wrap-input100">
						<span>Last Name</span> <input class="input100" type="text"
							style="border-bottom: 1px solid #555555; width: 30%"
							name="lastName" placeholder="Your answer">
					</div>

					<div class="wrap-input100">
						<span>Email ID <span class="bg1">*</span></span> <input
							style="border-bottom: 1px solid #555555; width: 30%"
							class="input100" type="text" name="emailId"
							placeholder="Your answer">
					</div>

					<div class="wrap-input100">
						<span>Contact Number <span class="bg1">*</span></span> <input
							style="border-bottom: 1px solid #555555; width: 30%"
							class="input100" type="number" name="contactNumber"
							placeholder="Your answer">
					</div>

					<div class="wrap-input100">
						<span>Current Profession</span> <input class="input100"
							style="border-bottom: 1px solid #555555; width: 30%" type="text"
							name="currentProfession" placeholder="Your answer">
					</div>

					<div class="wrap-input100">
						<span>Reason of Interest in Laundry</span> <input class="input100"
							style="border-bottom: 1px solid #555555; width: 30%" type="text"
							name="reasonOfInterest" placeholder="Your answer">
					</div>

					<div class="wrap-input100">
						<span>Why open a Franchise?</span> <input class="input100"
							style="border-bottom: 1px solid #555555; width: 30%" type="text"
							name="whyFranchise" placeholder="Your answer">
					</div>

					<div class="wrap-input100">
						<span>Have you ever experienced Leather Dry Clean laundry?</span>
						<input class="input100" type="text" name="haveYouExperienced"
							style="border-bottom: 1px solid #555555; width: 30%"
							placeholder="Your answer">
					</div>

					<div>
						<span><input type="submit" class="button" value="SUBMIT"></span>
						<span class="progressbar" style="border: 5px solid green"></span>
						<span style="float: right; clear: right"> Page 1 of 1</span>
					</div>
				</form>
				<p class="input100 wrap-input100">Never submit passwords through
					Google Forms.</p>
			</div>
		</div>
	</div>
</body>
</html>